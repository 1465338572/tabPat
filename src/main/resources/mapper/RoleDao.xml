<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.tabpat.dao.RoleDao">

    <select id="getRoleById" resultType="com.example.tabpat.domain.RoleDo">
        SELECT *
        FROM roles
        WHERE role_id = #{roleId};
    </select>
</mapper>